---
import {
  Carousel,
  CarouselContent,
  CarouselItem,
} from "@/components/ui/carousel"

const images = [
  { src: '/assetes/portfolio-1.jpg', alt: 'Image 1' },
  { src: '/assetes/portfolio-2.jpg', alt: 'Image 2' },
  { src: '/assetes/portfolio-3.jpg', alt: 'Image 3' },
  { src: '/assetes/portfolio-4.jpg', alt: 'Image 4' },
  { src: '/assetes/portfolio-5.jpg', alt: 'Image 5' },
  { src: '/assetes/portfolio-6.jpg', alt: 'Image 6' },
  { src: '/assetes/portfolio-7.jpg', alt: 'Image 7' },
  { src: '/assetes/portfolio-8.jpg', alt: 'Image 8' },
  { src: '/assetes/portfolio-9.jpg', alt: 'Image 9' },
  { src: '/assetes/portfolio-10.jpg', alt: 'Image 10' },
  { src: '/assetes/portfolio-11.jpg', alt: 'Image 11' },
  { src: '/assetes/portfolio-12.jpg', alt: 'Image 12' },
  { src: '/assetes/portfolio-13.jpg', alt: 'Image 13' },
  { src: '/assetes/portfolio-14.jpg', alt: 'Image 14' },
  { src: '/assetes/portfolio-15.jpg', alt: 'Image 15' },
  { src: '/assetes/portfolio-16.jpg', alt: 'Image 16' },
  { src: '/assetes/portfolio-17.jpg', alt: 'Image 17' },
  { src: '/assetes/portfolio-18.jpg', alt: 'Image 18' },
  { src: '/assetes/portfolio-19.jpg', alt: 'Image 19' },
  { src: '/assetes/portfolio-20.jpg', alt: 'Image 20' },
  { src: '/assetes/portfolio-21.jpg', alt: 'Image 21' },
  { src: '/assetes/portfolio-22.jpg', alt: 'Image 22' },
];
---

<Carousel orientation="vertical" client:load>
  <CarouselContent className="-ml-2 md:-ml-4" client:load>
  {images.map((image) => (
    <CarouselItem className="pl-2 md:pl-4" client:load>
      <img src={image.src} alt={image.alt} />
    </CarouselItem>
  ))}
  </CarouselContent>
</Carousel>